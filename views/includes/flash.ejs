
<% if(success.length) { %>
    <div class="alert alert-danger alert-dismissible fade show col-6 offset-3 " style="height: 3rem;align-content: center;;" role="alert">
    <%= success %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
    <% } %>

<% if(failure.length) { %>
    <div class="alert alert-danger alert-dismissible fade show col-6 offset-3 " style="height: 3rem;align-content: center;;" role="alert">
    <%= failure %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
    <% } %>